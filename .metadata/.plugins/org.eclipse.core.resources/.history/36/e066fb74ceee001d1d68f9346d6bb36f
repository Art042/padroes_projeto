package atividade_3.q_4.Factory;

public class LanchoneteCG implements Sanduiche {
	Carregando carregando = new Carregando();
    @Override
    public void adicionarIngredientes() {
        System.out.println("Saindo Sanduiche Especial CG");
        Carregando.barra_carregando('*', 5, 1000);
        System.out.println("Pão Integral");
        Carregando.barra_carregando('*', 5, 1000);
        System.out.println("Queijo Prato");
        Carregando.barra_carregando('*', 5, 1000);
        System.out.println("Presunto de Frango");
        
        
    }
}
