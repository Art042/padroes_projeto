package atividade_3.q_10.model.decorators;

import atividade_3.q_10.model.Pizza;

public class DoubleDrink extends DrinkDecorator {

	public DoubleDrink(Pizza drink) {
		super(drink);
	}

	@Override
	public void serve() {
		drink.serve();
		drink.serve();
	}

	@Override
	public Double getPrice() {
		return drink.getPrice() * 1.75d;
	}

}
