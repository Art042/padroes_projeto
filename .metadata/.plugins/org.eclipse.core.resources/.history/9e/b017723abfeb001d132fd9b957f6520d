package atividade_3.q_7;

public class Main {
    public static void main(String[] args) {
        BingoGame game = new BingoGame();
        game.addPlayer(new BingoPlayer("Alice", game));
        game.addPlayer(new BingoPlayer("Bob", game));
        game.addPlayer(new BingoPlayer("Charlie", game));
        game.start();
    }
}

