package atividade_3.q_1;

class Client {
    public static void main(String[] args) {
        Builder builder1 = new ContatoInternetBuilder();
        Builder builder2 = new ContatoTelefoneBuilder();
        Builder builder3 = new ContatoCompletoBuilder();

        Director director1 = new Director(builder1);
        Director director2 = new Director(builder2);
        Director director3 = new Director(builder3);

        director1.construirContato("João", "", "", "joao@mail.com");
        director2.construirContato("Maria", "", "555-1234", "");
        director3.construirContato("Pedro", "Rua 1, nº 2", "555-5678", "pedro@mail.com");

        System.out.println(director1.getContato());
        System.out.println(director2.getContato());
        System.out.println(director3.getContato());
    }
}