package atividade_3.q_7;

public class BingoNumber {
    private int value;
    private boolean marked;
    
    public BingoNumber(int value) {
        this.value = value;
        this.marked = false;
    }
    
    public int getValue() {
        return value;
    }
    
    public boolean isMarked() {
        return marked;
    }
    
    public void mark() {
        marked = true;
    }
}

