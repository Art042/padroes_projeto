package atividade_3.q_5;

import atividade_3.q_5.BrasilFactory.Brasil;

class LocaleFactory {
    public static AbstractFactory getFactory(String localeCode) {
        switch(localeCode) {
            case "BR":
                return new Brasil();
            case "US":
                return new EUA();
            default:
                throw new IllegalArgumentException("Código de localidade inválido.");
        }
    }
}