package atividade_3.q_10.model.decorators;

import atividade_3.q_10.model.Drink;

public class Bacon extends DrinkDecorator {
	
	public Bacon(Drink drink) {
		super(drink);
	}

	@Override
	public void serve() {
		drink.serve();
		System.out.println("- Adding 30ml of Milk");
	}

	@Override
	public Double getPrice() {
		return drink.getPrice() + 0.5d;
	}

}
