    	System.out.println("Saindo Sanduiche Especial RT");
    	Carregando.barra_carregando('*', 5, 900);
        System.out.println("Pão Bola");
        Carregando.barra_carregando('*', 5, 900);
        System.out.println("Queijo Cheddar");
        Carregando.barra_carregando('*', 5, 900);
        System.out.println("Presunto de Peru");
    }
}
