package atividade_3.q_7;

public class Main {
    public static void main(String[] args) {
        BingoGame game = new BingoGame();
        game.addPlayer(new BingoPlayer("Alice", game, 5));
        game.addPlayer(new BingoPlayer("Bob", game, 5));
        game.addPlayer(new BingoPlayer("Charlie", game, 5));
        game.start();
    }
}

