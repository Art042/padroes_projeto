package atividade_3.q_3.states;

import atividade_3.q_3.Processo;

public class Processando extends Status{

    public Processando(Processo pedido) {
        super(pedido);
    }

    @Override
    public String getStatus() {
        return "Processando o pedido";
    }

    @Override
    public void alterar() {
        // código para alteração do pedido
    }

    @Override
    public void cancelar() {
        pedido.changeStatus(new Cancelado(pedido));
    }

    @Override
    public void enviar() {
        pedido.changeStatus(new Enviado(pedido));
    }
}