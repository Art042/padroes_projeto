package atividade_3.q_10.model.decorators;

import atividade_3.q_10.model.Pizza;

public class DoubleDrink extends DrinkDecorator {

	public DoubleDrink(Pizza pizza) {
		super(pizza);
	}

	@Override
	public void serve() {
		pizza.serve();
		pizza.serve();
	}

	@Override
	public Double getPrice() {
		return pizza.getPrice() * 1.75d;
	}

}
