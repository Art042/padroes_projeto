package atividade_3.q_3.states;

import atividade_3.q_3.Processo;

public class Indeferido extends Status{

    public Indeferido(Processo processo) {
        super(processo);
    }

    @Override
    public String getStatus() {
        return "O processo foi Indeferido.\nProcesso Fechado";
    }

    @Override
    public void aberto() {
        String msg1 = "O processo foi Fechado.";
        throw new IllegalStateException(msg1);
    }

    @Override
    public void cancelar() {
        String msg1 = "O Processo já está Indeferido\nFechado";
        throw new IllegalStateException(msg1);
    }

    @Override
    public void deferido() {
        String msg1 = "O Processo foi Indeferido\nProcesso Fechado";
        throw new IllegalStateException(msg1);
    }
}
